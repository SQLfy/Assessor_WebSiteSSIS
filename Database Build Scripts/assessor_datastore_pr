	[subdivision_no] [varchar](30) NOT NULL,
	[subdivision_name] [varchar](256) NULL,
	[create_dtm] [datetime] NULL,
	[create_user_id] [varchar](30) NULL,
	[update_dtm] [datetime] NULL,
	[update_user_id] [varchar](30) NULL
) ON [PRIMARY]
