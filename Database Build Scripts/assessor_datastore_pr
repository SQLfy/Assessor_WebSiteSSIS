AS
SELECT [PERS_PROP_DETAIL_EID]
      ,[ACCOUNT_NO]
      ,[DETAIL_ID]
      ,[PERS_PROP_TYPE]
      ,[ASSET_TYPE_CODE]
      ,[ASSET_CONDITION_CODE]
      ,[ACQUIRED_YEAR]
      ,[QUANTITY]
      ,[ORIGINAL_COST]
      ,[ASSET_DETAIL_DESCR]
      ,[ROW_START_DTM]
      ,[ROW_END_DTM]
      ,[ROW_CURRENT_FLAG]
      ,[CREATE_DTM]
