CREATE TABLE [asr_staging].[s_tlkpsaleexclude](
	[EXCLUDECODE] [varchar](10) NULL,
	[EXCLUDEDESCRIPTION] [varchar](40) NULL,
	[SORTORDER] [numeric](9, 0) NULL,
	[ACTIVEFLAG] [numeric](1, 0) NULL,
	[JURISDICTIONID] [numeric](15, 0) NULL,
