	[ADJALTASSESSEDMINVALUE] [numeric](11, 0) NULL,
	[ADJALTASSESSEDDROUNDTYPE] [varchar](10) NULL,
	[ALTASSESSEDPRECISION] [numeric](11, 0) NULL,
	[ALTASSESSEDMINIMUMVALUE] [numeric](11, 0) NULL,
	[ALTASSESSEDROUNDTYPE] [varchar](10) NULL,
