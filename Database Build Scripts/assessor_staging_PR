	[LANDADJSIZE] [numeric](20, 6) NULL,
	[LANDOVERRIDEADJSIZE] [numeric](20, 6) NULL,
	[LANDABSTRACTON0] [numeric](20, 6) NULL,
	[LANDABSTRACTON1] [numeric](20, 6) NULL,
	[LANDABSTRACTON2] [numeric](20, 6) NULL,
