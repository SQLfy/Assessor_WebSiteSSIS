CREATE PROCEDURE [asr_api_test].[sp_AccountNeighborhoodData]

AS
	
BEGIN
	TRUNCATE TABLE asr_api_test.AccountNeighborhoodData

    INSERT INTO asr_api_test.AccountNeighborhoodData
