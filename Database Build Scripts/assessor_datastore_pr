      ,[PARCEL_NO]
      ,[LOT]
      ,[TRACT]
      ,[BLOCK]
      ,[CREATE_DTM]
      ,[CREATE_USER_ID]
      ,[UPDATE_DTM]
      ,[UPDATE_USER_ID]
  FROM [assessor_datastore].[asr_datastore].[account_platted_lot]


