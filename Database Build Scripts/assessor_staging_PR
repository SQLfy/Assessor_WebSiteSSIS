			FROM [asr_staging].[s_tbnsaleinventory]
			WHERE RECEPTIONNO IS NULL OR LEN(RECEPTIONNO) > 10;

------------------------------------------------------------------------------

   -- ACCOUNTNO - Not null and must exist in s_tblacct - FK check

