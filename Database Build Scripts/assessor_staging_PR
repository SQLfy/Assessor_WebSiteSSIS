                         JURISDICTIONID, PERSONOD0, PERSONOD1, PERSONOM0, PERSONOM1, PERSONOT0, PERSONOT1, WRITEDATE, SEQID, ETL_RUN_TIME
FROM            asr_staging.s_tblpersonsecure AS ps
