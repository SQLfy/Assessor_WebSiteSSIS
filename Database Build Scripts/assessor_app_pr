			,BOARD_NO						
			,APPEAL_DECISION				
			,SEQ_ID						
			,create_datetime				
			,create_user_id				
			,rowhash   					
            )
     VALUES (mySource.TAX_YEAR						
            ,mySource.APPEAL_NO						
	        ,mySource.APPEAL_TYPE					
	        ,mySource.APPEAL_METHOD					
	        ,mySource.ASSIGNED_TO					
