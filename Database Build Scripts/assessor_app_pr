			,SCHEDULE_START_TIME			
			,SCHEDULE_END_TIME				
			,SCHEDULE_COMMENT				
			,SCHEDULE_BOARD_FILE_NO		
			,SCHEDULE_APPRAISER_INITIALS	
			,APPOINTMENT_COLOR				
			,WRITE_DATE					
			,APPEAL_STATUS_ID				
