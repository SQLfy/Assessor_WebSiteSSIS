CREATE view [dqm].[q_tlkpleatype]
WITH SCHEMABINDING
AS 
select -- all columns
LEA, LEADESCRIPTION, SORTORDER, ACTIVEFLAG, JURISDICTIONID, LASTUPDATED
from [asr_staging].[s_tlkpleatype]
